                            <div class="content">
                                <div class="row">
                                    <div class="col-sm-4">
                                        <div class="icon-big text-center">
                                            <i class="maroon fas fa-user-check"></i>
                                        </div>
                                    </div>
                                    <div class="col-sm-8">
                                        <div class="detail">
                                            <p class="detail-subtitle">Active Members</p>
                                            <span class="number">{{countActiveUsers user}}</span>
                                        </div>
                                    </div>
                                </div>
                                <div class="footer">
                                    <hr />
                                    <div class="stats">
                                        <i class="fas fa-stopwatch"></i>All Time
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
